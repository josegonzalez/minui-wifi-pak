#!/bin/sh
bindir="$(dirname "$0")"
progdir="$(dirname "$bindir")"
[ -f "$progdir/debug" ] && set -x
PAK_NAME="$(basename "$progdir")"

main() {
    "$bindir/wifi-enable" &
}

if [ -f "$LOGS_PATH/$PAK_NAME.txt" ]; then
    mv "$LOGS_PATH/$PAK_NAME.txt" "$LOGS_PATH/$PAK_NAME.txt.old"
fi

main "$@" >"$LOGS_PATH/$PAK_NAME.txt" 2>&1
